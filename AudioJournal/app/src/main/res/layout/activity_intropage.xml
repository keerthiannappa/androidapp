<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.ganeshpartheeban.audiojournal.Intropage">

    <TextView
        android:id="@+id/AudioJournal"
        android:layout_width="149dp"
        android:layout_height="38dp"
        android:fontFamily="sans-serif"
        android:text="AudioJournal"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textColor="#b71c1c"
        tools:layout_editor_absoluteX="161dp"
        tools:layout_editor_absoluteY="379dp"
        android:layout_marginBottom="103dp"
        android:layout_marginEnd="86dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="48dp"
        android:layout_height="51dp"
        app:srcCompat="@drawable/audiojournalicon"
        tools:layout_editor_absoluteX="99dp"
        tools:layout_editor_absoluteY="366dp"
        android:layout_alignBottom="@+id/AudioJournal"
        android:layout_toStartOf="@+id/AudioJournal" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/imageView"
        android:layout_marginEnd="40dp"
        android:text="by   Ganesh Partheeban" />
</RelativeLayout>
